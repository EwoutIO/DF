<DesignPattern>
	<Name>Singleton</Name>
	<Components>
		<Component>
			<Name>Singleton</Name>
			<ComponentType>Class</ComponentType>
			<AccessModifier>Public</AccessModifier>
			<Members>
				<Member>
					<MemberType>Constructor</MemberType>
					<AccessModifier>Private</AccessModifier>
					<Occurrence>
						<MinimumOccurrence>1</MinimumOccurrence>
						<MaximumOccurrence>N</MaximumOccurrence>
					</Occurrence>
				</Member>
				<Member>
					<MemberType>Constructor</MemberType>
					<AccessModifier>Public</AccessModifier>
					<Occurrence>
						<MinimumOccurrence>0</MinimumOccurrence>
						<MaximumOccurrence>0</MaximumOccurrence>
					</Occurrence>
				</Member>
				<Member>
					<MemberType>Method</MemberType>
					<AccessModifier>Public</AccessModifier>
					<Modifier>Static</Modifier>
					<DataType>
						<Collection>false</Collection>
						<Custom>true</Custom>
						<TypeName>Singleton</TypeName>
					</DataType>
					<Occurrence>
						<MinimumOccurrence>1</MinimumOccurrence>
						<MaximumOccurrence>N</MaximumOccurrence>
					</Occurrence>
				</Member>
			</Members>
			<Occurrence>
				<MinimumOccurrence>1</MinimumOccurrence>
				<MaximumOccurrence>1</MaximumOccurrence>
			</Occurrence>
		</Component>
	</Components>
</DesignPattern>
